{
  "name": "screenwtf",
  "productName": "screenwtf",
  "version": "1.0.0",
  "description": "Cloud screenshot uploader, uses OS specific screenshotting tool",
  "main": "src/index.js",
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "lint": "echo \"No linting configured\""
  },
  "keywords": [],
  "author": {
    "name": "Eric Scott",
    "email": "eric.scott@textlocal.com"
  },
  "license": "MIT",
  "config": {
    "forge": {
      "packagerConfig": {},
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel"
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        }
      ]
    }
  },
  "dependencies": {
    "axios": "^0.19.2",
    "electron-squirrel-startup": "^1.0.0",
    "node-notifier": "^6.0.0",
    "request": "^2.88.0"
  },
  "devDependencies": {
    "@electron-forge/cli": "6.0.0-beta.49",
    "@electron-forge/maker-deb": "6.0.0-beta.49",
    "@electron-forge/maker-rpm": "6.0.0-beta.49",
    "@electron-forge/maker-squirrel": "6.0.0-beta.49",
    "@electron-forge/maker-zip": "6.0.0-beta.49",
    "electron": "8.0.0"
  }
}
